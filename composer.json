{
    "name": "luoyy/aliyun-core",
    "description": "aliyun-php-sdk-core",
    "require": {
        "php": ">=7.1.0"
    },
    "authors": [{
        "name": "luoyy",
        "email": "zishang520@gmail.com"
    }],
    "keywords": ["aliyun", "core", "laravel", "lumen"],
    "support": {
        "issues": "https://github.com/zishang520/aliyun-core/issues",
        "source": "https://github.com/zishang520/aliyun-core"
    },
    "autoload": {
        "psr-4": {
            "luoyy\\AliCore\\": "src/"
        }
    },
    "extra": {
